# BuildOS - Production Environment Variables
# Copy this file to .env and fill in the values

# Database
POSTGRES_USER=buildos
POSTGRES_PASSWORD=CHANGE_ME_STRONG_PASSWORD
POSTGRES_DB=buildos_prod

# Redis
REDIS_PASSWORD=CHANGE_ME_REDIS_PASSWORD

# MinIO (S3-compatible storage)
MINIO_ROOT_USER=buildos
MINIO_ROOT_PASSWORD=CHANGE_ME_MINIO_PASSWORD

# Next.js
NEXTAUTH_SECRET=CHANGE_ME_NEXTAUTH_SECRET_32_CHARS_MIN
NEXTAUTH_URL=https://buildos.designcorp.eu

# Image tag (for docker-compose)
IMAGE_TAG=latest
